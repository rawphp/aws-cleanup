{
    "name": "rawphp/aws-cleanup",
    "description": "AWS Cleanup tool",
    "keywords": ["aws", "php", "clean"],
    "homepage": "https://github.com/rawphp/aws-cleanup",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/rawphp/aws-cleanup/issues",
        "source": "https://github.com/rawphp/aws-cleanup"
    },
    "authors": [
        {
            "name": "Tom Kaczocha",
            "email": "tomkaczocha@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "aws/aws-sdk-php": "^3.293",
        "laminas/laminas-text": "^2.5",
        "laravel-zero/phar-updater": "^1.3",
        "nunomaduro/termwind": "^1.15.1"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "laravel-zero/framework": "^10.2",
        "mockery/mockery": "^1.6",
        "pestphp/pest": "^2.22",
        "pestphp/pest-plugin-watch": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["builds/aws-cleanup"]
}
